QListWidget {
  font: 14px;
  outline: none;
  border: none;
}

QListWidget::item {
  border: none;
  outline: none;
  color: rgb(248, 248, 242);
  padding: 10px 20px 15px 20px;
}

QListWidget::item:hover {
  color: rgb(189, 147, 249);
  background-color: rgb(68, 71, 90);
}

QListWidget::item:focus {
  color: rgb(189, 147, 249);
  background-color: rgb(68, 71, 90);
}
